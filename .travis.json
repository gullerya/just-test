{
	"language": "node_js",
	"os": [
		"linux"
	],
	"dist": "xenial",
	"node_js": [
		"14"
	],
	"install": [
		"npm install"
	],
	"jobs": {
		"include": [
			{
				"stage": "qualify",
				"name": "lint",
				"script": [
					"npm run lint"
				]
			},
			{
				"stage": "qualify",
				"name": "test",
				"script": [
					"npm run test && bash <(curl -s https://codecov.io/bash)"
				]
			}
		]
	},
	"cache": {
		"directories": [
			"node_modules"
		]
	}
}